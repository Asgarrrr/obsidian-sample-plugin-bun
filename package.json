{
	"name": "obsidian-sample-plugin-bun",
	"version": "1.0.0",
	"author": "",
	"main": "src/main.ts",
	"devDependencies": {
		"@biomejs/biome": "2.2.4",
		"@types/bun": "^1.2.22",
		"builtin-modules": "3.3.0",
		"obsidian": "latest",
		"obsidian-typings": "^4.50.0",
		"tslib": "2.4.0"
	},
	"description": "This is a sample plugin for Obsidian (https://obsidian.md) with bun support.",
	"keywords": [],
	"license": "MIT",
	"scripts": {
		"dev": "bun bun.build.ts",
		"build": "bun bun.build.ts production",
		"version": "bun version-bump.mjs && git add manifest.json versions.json",
		"lint": "bunx biome lint src/",
		"lint:fix": "bunx biome lint --write src/",
		"format": "bunx biome format --write src/",
		"type-check": "bunx tsc --noEmit",
		"clean": "bun rm -rf main.js *.tsbuildinfo"
	}
}
