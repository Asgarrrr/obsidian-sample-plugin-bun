{
	"name": "obsidian-sample-plugin-bun",
	"version": "1.0.0",
	"description": "This is a sample plugin for Obsidian (https://obsidian.md) with bun support.",
	"main": "src/main.ts",
	"scripts": {
		"dev": "bun esbuild.config.mjs",
		"build": "bun esbuild.config.mjs production",
		"version": "bun version-bump.mjs && git add manifest.json versions.json",
		"lint": "bunx eslint src/",
		"lint:fix": "bunx eslint src/ --fix",
		"type-check": "bunx tsc --noEmit",
		"clean": "bun rm -rf main.js *.tsbuildinfo"
	},
	"keywords": [],
	"author": "",
	"license": "MIT",
	"devDependencies": {
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"obsidian": "latest",
		"tslib": "2.4.0"
	},
	"dependencies": {
		"bun-types": "^1.2.12",
		"obsidian-typings": "^3.7.0"
	}
}
